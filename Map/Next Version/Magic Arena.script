        Actions.MapTileObjectSet(aX, aY, 255);
      end;
    end;

    Module := 58;
    if aObj = RUNE_MAGIC_IMMUNITY_OBJ then
    begin
      if States.GameTime > Players[P].HeroMagicImmunityTime then

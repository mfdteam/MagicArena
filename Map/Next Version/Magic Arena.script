    end;

    Players[P].LastUsedSkill := sRocket;

    Players[P].LastRoadSkill := sRocket;

    if Players[P].RocketCharges > 0 then
    begin
      Players[P].RocketCharges := Players[P].RocketCharges - 1 * Byte(Players[P].HeroType <> 25);

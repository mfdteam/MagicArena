        end;
        if TileOnRuneSpawn(aX, aY) then
          RuneTaken := True;
        Actions.MapTileObjectSet(aX, aY, 255);
      end;
    end;

    Module := 59;
    if aObj = RUNE_ILLUSION_OBJ then

    end;

    Module := 57;
    if aObj = RUNE_DOUBLE_OBJ then
    begin
      if States.GameTime > Players[P].DDTime then
      begin
        Players[P].DD := True;
        Players[P].DDTime := States.GameTime + RUNE_DOUBLE_TIME;
        if TileOnRuneSpawn(aX, aY) then
